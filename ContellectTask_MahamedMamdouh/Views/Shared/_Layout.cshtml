<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ContellectTask_MahamedMamdouh</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/font/bootstrap-icons.css" rel="stylesheet" />
</head>
<body>
    <nav class="box mb-5 navbar">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">
                <img src="/download.png" style="width: 200px;">
            </a>
            <a asp-controller="Home"
               asp-action="Index" class="font-weight-bolder text-danger box btn"><i class="bi bi-box-arrow-left mx-1"></i>Logout</a>
        </div>
    </nav>


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
